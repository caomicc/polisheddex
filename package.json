{
  "name": "polisheddex",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "node scripts/generate-build-info.mjs & tsx scripts/smart-sprite-build.ts & ((tsx scripts/create-manifests.ts && ./scripts/copy-dex-files.sh) && next build)",
    "build-full": "node scripts/generate-build-info.mjs & npm run process-sprites & ((tsx scripts/create-manifests.ts && ./scripts/copy-dex-files.sh) && next build)",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "extract": "ts-node extract_pokemon_data.ts && ts-node extract_locations.ts",
    "extract-rival": "ts-node extract_rival.ts",
    "generate-individual-files": "npx tsx scripts/generate-individual-pokemon-files.ts",
    "optimize": "npm run generate-individual-files && npm run create-manifests && npm run compress-data",
    "create-manifests": "ts-node scripts/create-manifests.ts",
    "compress-data": "ts-node scripts/compress-pokemon-data.ts",
    "do-the-thing": "npm run extract && npm run optimize && npx tsx restructure_locations.ts && npm run process-sprites && sh scripts/copy-dex-files.sh && npm run generate-individual-files",
    "crop": "ts-node scripts/crop_top_sprite.ts",
    "process-sprites": "python process_sprites.py --all",
    "process-pokemon": "python process_sprites.py --pokemon",
    "process-trainers": "python process_sprites.py --trainers",
    "update-rom": "cd polishedcrystal && git pull origin master && cd ..",
    "test": "vitest run",
    "test:watch": "vitest",
    "analyze": "ANALYZE=true npm run build"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tanstack/react-table": "^8.21.3",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.525.0",
    "next": "15.3.5",
    "next-themes": "^0.4.6",
    "nuqs": "^2.4.3",
    "pngjs": "^7.0.0",
    "radix-ui": "^1.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "recharts": "^2.15.4",
    "remark-gfm": "^4.0.1",
    "remark-wiki-link": "^2.0.1",
    "sharp": "^0.34.2",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.4.5",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pngjs": "^6.0.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "lightningcss": "^1.30.1",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5.8.3",
    "vitest": "^0.34.6"
  }
}
